  "babel": {
    "presets": [
      "stage-1",
      [
        "env",
        {
          "targets": {
            "browsers": [
              "last 2 versions"
            ]
          },
          "modules": false
        }
      ],
      "react"
    ]
  },
  "eslintConfig": {
    "extends": [
      "airbnb"
    ],
    "env": {
      "browser": true
    },
    "rules": {
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      ],
      "react/prefer-stateless-function": [
        "off"
      ],
      "react/prop-types": [
        "off"
      ],
      "import/prefer-default-export": [
        "off"
      ],
      "arrow-body-style": [
        "off"
      ],
      "jsx-a11y/no-static-element-interactions": [
        "off"
      ],
      "object-shorthand": [
        "off"
      ]
    }
  }
